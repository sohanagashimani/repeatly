generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Job {
  id        String   @id @default(uuid())
  name      String
  cron      String
  url       String
  method    String
  headers   Json?
  body      Json?
  nextRun   DateTime
  retries   Int      @default(0)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
} 